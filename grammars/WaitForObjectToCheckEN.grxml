<?xml version="1.0" encoding="utf-8"?>
<grammar version="1.0" xml:lang="en-US" root="rootRule" tag-format="semantics/1.0-literals" xmlns="http://www.w3.org/2001/06/grammar">
	<rule id="rootRule">
		<one-of>
			<item><ruleref uri="#Hand"/></item>
			<item><ruleref uri="#Torso"/></item>
			<item><ruleref uri="#Stop"/></item>
			<item><ruleref uri="#Next"/></item>
			<item><ruleref uri="#Color"/></item>
		</one-of>
	</rule>
  
    <rule id="Hand" scope="private">
        <tag>HAND</tag>
        <one-of>
			<item>EMYS</item>
			<item></item>
        </one-of>
        <one-of>
			<item>look</item>
			<item>take a look</item>
			<item>take a peek</item>
			<item>watch</item>
        </one-of>
        <one-of>
			<item>look</item>
			<item>take a look</item>
			<item>take a peek</item>
			<item>watch</item>
        </one-of>
        <one-of>
			<item>please</item>
			<item></item>
        </one-of>
    </rule>
	
 	
    <rule id="Torso" scope="private">
        <tag>TORSO</tag>
        <one-of>
			<item>EMYS</item>
			<item></item>
        </one-of>
        <one-of>
			<item>look</item>
			<item>take a look</item>
			<item>take a peek</item>
			<item>watch</item>
        </one-of>
        <one-of>
			<item>at my clothes</item>
			<item>my clothes</item>
			<item>at my shirt</item>
			<item>my shirt</item>
			<item>at my pullover</item>
			<item>at my blouse</item>
			<item>at me</item>
			<item>at my sweater</item>
			<item>at my jacket</item>
			<item>at my coat</item>
			<item>at my stomach</item>
			<item>at my abdomen</item>
			<item>at my tee shirt</item>
        </one-of>
        <one-of>
			<item>please</item>
			<item></item>
        </one-of>
    </rule>	 
	
    <rule id="Stop" scope="private">
        <tag>STOP</tag>
        <one-of>
			<item>EMYS</item>
			<item></item>
        </one-of>
        <one-of>
			<item>ok now</item>
			<item>ok</item>
			<item></item>
        </one-of>
        <one-of>
			<item>enough</item>
			<item>stop</item>
			<item>this is enough</item>
			<item>that's all</item>
			<item>stop it</item>
			<item>finish</item>
			<item>let's end</item>
        </one-of>
        <one-of>
			<item>this</item>
			<item>this game</item>
			<item>this now</item>
			<item>this game now</item>
			<item>now</item>
			<item></item>
        </one-of>
    </rule>	
	
	
    <rule id="Next" scope="private">
        <tag>NEXT</tag>
        <one-of>
			<item>EMYS</item>
			<item></item>
        </one-of>
        <one-of>
			<item>ok</item>
			<item></item>
        </one-of>
