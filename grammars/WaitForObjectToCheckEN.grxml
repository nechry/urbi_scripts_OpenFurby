			<item>game</item>
			<item></item>
        </one-of>
    </rule>	
	
    <rule id="Color" scope="private">
        <tag>COLOR</tag>
        <one-of>
			<item>EMYS</item>
			<item>and</item>
			<item></item>
        </one-of>
        <one-of>
			<item>what is this</item>
			<item>do you know this</item>
			<item>do you know this one</item>
			<item>what</item>
			<item>this</item>
			<item>what is</item>
        </one-of>
        <one-of>
			<item>color</item>
			<item>color this is</item>
			<item></item>
        </one-of>
    </rule>		
	
</grammar>